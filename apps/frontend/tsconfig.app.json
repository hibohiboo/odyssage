{
  "extends": "@odyssage/tsconfig/tsconfig.frontend",
  "compilerOptions": {
    "paths": { 
      "@odyssage/frontend/*": ["./src/*"],
      "@odyssage/core/*": ["../../packages/core/*"],
      "@odyssage/backend/*": ["../backend/src/*"]
     },
     "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
     "allowSyntheticDefaultImports": true
  },
  "include": ["src","../../packages/core/**/*.ts","types","../backend/src/**/*.ts", "../backend/worker-configuration.d.ts"] ,
  "exclude": ["node_modules", "dist"]
} 